
public class Television {

	private int volume;
	private int canal;
	
	public Television(int volume,int canal) {
		this.volume=volume;
		this.canal=canal;
		
	}
	//getters e setters
	public int getVolume() {
		return volume;
	}
	
	public void setVolume(int volume) {
		this.volume=volume;
	}
	
	//getters e setters
	public int getCanal() {
		return canal;
	}
	
	public void setCanal(int canal) {
		this.canal=canal;
	}
}
